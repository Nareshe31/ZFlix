{
  "expo": {
    "name": "ZFlix",
    "slug": "ZFlix",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/custom-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0608"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.naresh.ZFlix"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Watch movies online for free and browse torrents for movies and series"
  }
}
